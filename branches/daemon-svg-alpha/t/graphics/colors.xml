  <section refnum="6" xml:id="S6">
    <title><tag>6 </tag>Math</title>
    <para xml:id="S6.p1">
      <equation xml:id="S6.Ex1">
        <Math mode="display" xml:id="S6.Ex1.m1" tex="{\color{red}R}+{\color{green}G}+{\color{blue}B}" text="R + G + B">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="UNKNOWN" color="red" font="italic">R</XMTok>
              <XMTok role="UNKNOWN" color="green" font="italic">G</XMTok>
              <XMTok role="UNKNOWN" color="blue" font="italic">B</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S6.p2">
      <equation xml:id="S6.Ex2">
        <Math mode="display" xml:id="S6.Ex2.m1" tex="{\pagecolor{red}R}+{\pagecolor{green}G}+{\pagecolor{blue}B}" text="R + G + B">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="UNKNOWN" backgroundcolor="red" font="italic">R</XMTok>
              <XMTok role="UNKNOWN" backgroundcolor="green" font="italic">G</XMTok>
              <XMTok role="UNKNOWN" backgroundcolor="blue" font="italic">B</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S6.p3">
      <equation xml:id="S6.Ex3">
        <Math mode="display" xml:id="S6.Ex3.m1" tex="\fcolorbox{green}{blue}{\textcolor{red}{R}}+\fcolorbox{blue}{red}{\textcolor{green}{G}}+\fcolorbox{red}{green}{\textcolor{blue}{B}}" text="[R] + [G] + [B]">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMText>
                <text framecolor="green" framed="rectangle" color="red" backgroundcolor="blue">R</text>
              </XMText>
              <XMText>
                <text framecolor="blue" framed="rectangle" color="green" backgroundcolor="red">G</text>
              </XMText>
              <XMText>
                <text framecolor="red" framed="rectangle" color="blue" backgroundcolor="green">B</text>
              </XMText>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
